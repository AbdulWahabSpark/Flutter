/**
    DateTimeEvent
 **////
abstract class DateTimeEvent {}
class _DateTime extends DateTimeEvent {
  DateTime? slectedDate = DateTime.now();
  _DateTime(this.slectedDate){}
}
class DateTimeState {
  DateTime? slectedDate = DateTime.now();
  DateTimeState(this.slectedDate){}
}

class DateTimeBloc extends Bloc<DateTimeEvent, DateTimeState> {
  DateTime? slectedDate = DateTime.now();
  DateTimeBloc() : super(DateTimeState(DateTime.now())) {
    on<_DateTime>(mapEventToState);
  }
  void mapEventToState(DateTimeEvent event, Emitter<DateTimeState> emit) async {
    if (event is _DateTime) {
      slectedDate = event.slectedDate;
    }
    /*(event, emit) => */emit(DateTimeState(slectedDate));
  }
  }

