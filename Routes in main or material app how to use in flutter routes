***Routes

MaterialApp(
        theme: ThemeData(
          primarySwatch: Colors.blue,
          visualDensity: VisualDensity.adaptivePlatformDensity,
        ),
        home: const SplashScreen(),
        routes: <String, WidgetBuilder>{
          SPLASH_SCREEN: (BuildContext context) => const SplashScreen(),
          WELCOME_SCREEN: (BuildContext context) => WelcomePage(),
          LOGIN_SCREEN: (BuildContext context) => SignInPage(),
          DASHBOARD_SCREEN: (BuildContext context) => const DashboardPage(),
          CLAIMDETAIL_SCREEN: (BuildContext context) => ClaimDetailPage(),
          REGISTER1_SCREEN: (BuildContext context) => Register1Page(),
          REGISTER2_SCREEN: (BuildContext context) => Register2Page(),
          REGISTER3_SCREEN: (BuildContext context) => Register3Page(),
          REGISTER4_SCREEN: (BuildContext context) => Register4Page(),
          REGISTER5_SCREEN: (BuildContext context) => Register5Page(),
          EDITPROFILE_SCREEN: (BuildContext context) => EditProfilePage(),
          MovieList_SCREEN: (BuildContext context) => MovieList(),
        },
      ),
